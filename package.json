{
    "name": "tellie",
    "version": "0.0.1",
    "description": "",
    "main": "dist/tellie.js",
    "scripts": {
        "build:bot": "netlify-lambda -c ./webpack.config.js build src/bot",
        "build:site": "cpx src/site/**/* dist",
        "build": "npm-run-all build:bot build:site",
        "ngrok": "ngrok http -bind-tls=true 8080",
        "dev:bot": "netlify-lambda -c ./webpack.config.js serve src/bot",
        "dev:site": "live-server src/site --port=8080 --no-browser --proxy=/.netlify/functions:http://localhost:9000/.netlify/functions"
    },
    "author": "Niels Bik <hey@nielsbik.nl> (https://nielsbik.nl)",
    "license": "MIT",
    "files": [
        "src",
        "dist"
    ],
    "devDependencies": {
        "@babel/preset-typescript": "^7.1.0",
        "@types/dotenv": "^6.1.0",
        "@types/express": "^4.16.0",
        "@types/lodash": "^4.14.119",
        "@types/node-fetch": "^2.1.4",
        "cpx": "^1.5.0",
        "dotenv": "^6.2.0",
        "live-server": "^1.2.1",
        "netlify-lambda": "^1.1.1",
        "ngrok": "^3.1.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.15.3"
    },
    "dependencies": {
        "chrono-node": "^1.3.5",
        "date-fns": "^1.30.1",
        "date-fns-timezone": "^0.1.4",
        "express": "^4.16.4",
        "lodash": "^4.17.11",
        "node-fetch": "^2.3.0",
        "serverless-http": "^1.8.0",
        "telegraf": "^3.25.5",
        "telegraf-command-parts": "^1.0.3"
    }
}
